{
	"name": "chillerlan/php-magic-apiclient",
	"description": "A magic API client for PHP7.4+.",
	"homepage": "https://github.com/chillerlan/php-magic-apiclient",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "stable",
	"keywords": [
		"api", "http", "client", "magic"
	],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-magic-apiclient/issues",
		"source": "https://github.com/chillerlan/php-magic-apiclient"
	},
	"require": {
		"php": "^7.4 || ^8.0",
		"ext-json": "*",
		"chillerlan/php-settings-container":"^2.1",
		"psr/http-message":"^1.0"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\HTTP\\MagicAPI\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\HTTPTest\\MagicAPI\\": "tests/"
		}
	}
}
